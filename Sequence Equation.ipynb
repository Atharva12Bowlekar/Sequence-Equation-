   "source": [
    "def permutationEquation(p):\n",
    "    n=len(p)\n",
    "    s=[]\n",
    "    t=[]\n",
    "    for i in range(1,n+1):\n",
    "        if i in p:\n",
    "            a=p.index(i)\n",
    "            s.append(a+1)\n",
    "    for i in s:\n",
    "        a=p.index(i)\n",
    "        t.append(a+1)\n",
    "    return t"
   ]
  },
